{
  "name": "splitio/split-sdk-php",
  "description": "Split SDK for PHP",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Split",
      "homepage": "http://split.io"
    },
    {
      "name": "Sebastian Arrubia",
      "email": "sarrubia@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "ext-curl": "*",
    "ext-redis": "*",
    "ext-mbstring": "*",
    "psr/log": "~1.0",
    "psr/cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpdocumentor/phpdocumentor": "2.*",
    "squizlabs/php_codesniffer": "2.*",
    "fabpot/php-cs-fixer": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "SplitIO\\": "src/SplitIO/"
    },
    "files": ["src/functions_include.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "SplitIO\\Test\\": "tests/"
    }
  }
}
